{
  "type": "extension",
  "name": "vessels_bridge",
  "version": "0.1.0",
  "description": "Bridge between TEN Framework real-time audio graph and Vessels agentic logic",
  "language": "python",
  "dependencies": [],
  "api": {
    "cmd_in": [
      {
        "name": "flush"
      }
    ],
    "cmd_out": [
      {
        "name": "flush"
      }
    ],
    "data_in": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          },
          "is_final": {
            "type": "bool"
          },
          "stream_id": {
            "type": "uint32"
          },
          "end_of_segment": {
            "type": "bool"
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          }
        }
      }
    ],
    "audio_frame_in": [],
    "audio_frame_out": [],
    "video_frame_in": [],
    "video_frame_out": []
  },
  "property": {
    "user_id": {
      "type": "string",
      "default": "default_user"
    },
    "community_id": {
      "type": "string",
      "default": "default_community"
    },
    "vessel_id": {
      "type": "string",
      "default": ""
    },
    "context": {
      "type": "string",
      "default": "voice_interface"
    },
    "language": {
      "type": "string",
      "default": "en"
    },
    "log_level": {
      "type": "string",
      "default": "info"
    }
  }
}
